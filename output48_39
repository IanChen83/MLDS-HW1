import theano
import theano.tensor as T
import numpy as np
__author__= 'jason'

class MAP:
	"""docstring for ClassName"""
	def __init__(self):
		self.map_data = open('hw1/MLDS_HW1_RELEASE_v1/phones/48_39.map','r')
		self.in_48 = []
		self.in_39 = []	
		for line in self.map_data:
			in_x = line.split('\t')
			self.in_48.append(in_x[0])
			self.in_39.append(in_x[1])	

	def map(self,y):
		big = max(y)
		big_index = y.index(big)

		return self.in_39[big_index]

#m = MAP()
